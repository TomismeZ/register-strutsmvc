<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.5//EN" "http://struts.apache.org/dtds/struts-2.5.dtd">
<struts>
	<package name="register" extends="struts-default">
		<action name="register" class="com.qingshixun.action.RegisterAction" method="register">
			<result name="success">/WEB-INF/views/register.jsp</result>
		</action>
		<action name="registerResult" class="com.qingshixun.action.RegisterAction" method="registerResult">
			<result name="success">/WEB-INF/views/success.jsp</result>
			<result name="error">/WEB-INF/views/register.jsp</result>
		</action>
	</package>
</struts>